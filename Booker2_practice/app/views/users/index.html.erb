<%= render 'users/list',book:@book_new,user:@user%>
<div class="col-xs-9">
	<h2>Users</h2>
	<table class="table">
		<thead>
			<tr>
				<th>image</th>
				<th>name</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= attachment_image_tag user,:profile_image ,fallback:'no_image.jpg', format:'jpeg',size:'40x40' %></td>
					<td><%= user.name%></td>
					<td><%= link_to "show" ,user_path(user.id)%></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>